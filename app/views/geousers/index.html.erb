<p id="notice"><%= notice %></p>

<h1>Listing Geousers</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @geousers.each do |geouser| %>
      <tr>
        <td><%= geouser.title %></td>
        <td><%= geouser.description %></td>
        <td><%= geouser.address %></td>
        <td><%= geouser.latitude %></td>
        <td><%= geouser.longitude %></td>
        <td><%= link_to 'Show', geouser %></td>
        <td><%= link_to 'Edit', edit_geouser_path(geouser) %></td>
        <td><%= link_to 'Destroy', geouser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Geouser', new_geouser_path %>
